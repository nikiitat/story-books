{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": ["google", "prettier"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error",
        "func-names": "off",
        "require-jsdoc": "off",
        "camelcase": "off"
        // 'semi': ['error', 'never'],
        // 'quotes': ['error', 'single'],
        // 'indent': ['error', 4],
        // 'object-curly-spacing': [2, 'always'],
        // 'max-len': [2, 80, 4, { 'ignoreUrls': true }],
        // 'comma-dangle': ['error', 'never']
    }
}
